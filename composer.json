{
    "name":"gtck520/konger-weibo",
    "description": "the new thinkphp project",
    "type": "project",
    "authors": [
        {
            "name": "konger",
            "email": "496317580@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0.0",
        "topthink/think-orm": "^2.0",
        "workerman/gateway-worker": "^3.0",
        "workerman/mysql": "^1.0",
        "overtrue/wechat": "~4.0",
        "workerman/gatewayclient": "^3.0"
    },
    "require-dev": {
